<?xml version="1.0"?>
<robot name="humanoid_planar_ref_proportions">

  <!-- Material definitions -->
  <material name="torso_main"> <color rgba="0.3 0.3 0.8 1"/> </material>
  <material name="torso_acc">  <color rgba="0.5 0.5 0.9 1"/> </material>
  <material name="limb_upper"> <color rgba="0.7 0.7 0.7 1"/> </material>
  <material name="limb_lower"> <color rgba="0.5 0.5 0.5 1"/> </material>
  <material name="head">       <color rgba="0.9 0.9 0.9 1"/> </material>
  <material name="foot">       <color rgba="0.2 0.2 0.2 1"/> </material>

  <!-- TORSO: Main body link -->
  <link name="torso">
    <inertial>
      <mass value="12.0"/>
      <origin xyz="0 0 0.30" rpy="0 0 0"/>
      <inertia ixx="0.1667" ixy="0" ixz="0" iyy="0.1667" iyz="0" izz="0.0600"/>
    </inertial>
    <visual>
      <origin xyz="0 0 0.30" rpy="0 0 0"/>
      <geometry><capsule radius="0.12" length="0.50"/></geometry>
      <material name="torso_main"/>
    </visual>
    <collision>
      <origin xyz="0 0 0.30" rpy="0 0 0"/>
      <geometry><capsule radius="0.12" length="0.50"/></geometry>
      <contact_coefficients mu="0.9" kp="1e6" kd="100"/>
    </collision>
  </link>

  <!-- HEAD -->
  <link name="head">
    <inertial>
      <mass value="2.5"/>
      <origin xyz="0 0 0"/>
      <inertia ixx="0.0125" ixy="0" ixz="0" iyy="0.0125" iyz="0" izz="0.0125"/>
    </inertial>
    <visual>
      <origin xyz="0 0 0"/>
      <geometry><sphere radius="0.12"/></geometry>
      <material name="head"/>
    </visual>
    <collision>
      <origin xyz="0 0 0"/>
      <geometry><sphere radius="0.12"/></geometry>
    </collision>
  </link>
  <joint name="j_head" type="revolute">
    <parent link="torso"/>
    <child link="head"/>
    <origin xyz="0 0 0.62" rpy="0 0 0"/>
    <axis xyz="0 1 0"/>
    <limit lower="-0.5" upper="0.5" effort="50" velocity="5.0"/>
    <dynamics damping="5.0" friction="0.5"/>
  </joint>

  <!-- LEFT ARM -->
  <link name="upperarm_l">
    <inertial>
      <mass value="1.8"/>
      <origin xyz="0 0 -0.15"/>
      <inertia ixx="0.0068" ixy="0" ixz="0" iyy="0.0068" iyz="0" izz="0.0018"/>
    </inertial>
    <visual>
      <origin xyz="0 0 -0.15" rpy="1.5708 0 0"/>
      <geometry><capsule radius="0.05" length="0.24"/></geometry>
      <material name="limb_upper"/>
    </visual>
    <collision>
      <origin xyz="0 0 -0.15" rpy="1.5708 0 0"/>
      <geometry><capsule radius="0.05" length="0.24"/></geometry>
    </collision>
  </link>
  <joint name="j_shoulder_l" type="revolute">
    <parent link="torso"/>
    <child link="upperarm_l"/>
    <origin xyz="0.13 0 0.53" rpy="0 0 0"/>
    <axis xyz="0 1 0"/>
    <limit lower="-2.6" upper="2.6" effort="100" velocity="10.0"/>
    <dynamics damping="10.0" friction="1.0"/>
  </joint>

  <link name="forearm_l">
    <inertial>
      <mass value="1.2"/>
      <origin xyz="0 0 -0.12"/>
      <inertia ixx="0.0046" ixy="0" ixz="0" iyy="0.0046" iyz="0" izz="0.0012"/>
    </inertial>
    <visual>
      <origin xyz="0 0 -0.12" rpy="1.5708 0 0"/>
      <geometry><capsule radius="0.045" length="0.20"/></geometry>
      <material name="limb_lower"/>
    </visual>
    <collision>
      <origin xyz="0 0 -0.12" rpy="1.5708 0 0"/>
      <geometry><capsule radius="0.045" length="0.20"/></geometry>
    </collision>
  </link>
  <joint name="j_elbow_l" type="revolute">
    <parent link="upperarm_l"/>
    <child link="forearm_l"/>
    <origin xyz="0 0 -0.30" rpy="0 0 0"/>
    <axis xyz="0 1 0"/>
    <limit lower="0" upper="2.7" effort="80" velocity="10.0"/>
    <dynamics damping="8.0" friction="0.8"/>
  </joint>

  <!-- RIGHT ARM -->
  <link name="upperarm_r">
    <inertial>
      <mass value="1.8"/>
      <origin xyz="0 0 -0.15"/>
      <inertia ixx="0.0068" ixy="0" ixz="0" iyy="0.0068" iyz="0" izz="0.0018"/>
    </inertial>
    <visual>
      <origin xyz="0 0 -0.15" rpy="1.5708 0 0"/>
      <geometry><capsule radius="0.05" length="0.24"/></geometry>
      <material name="limb_upper"/>
    </visual>
    <collision>
      <origin xyz="0 0 -0.15" rpy="1.5708 0 0"/>
      <geometry><capsule radius="0.05" length="0.24"/></geometry>
    </collision>
  </link>
  <joint name="j_shoulder_r" type="revolute">
    <parent link="torso"/>
    <child link="upperarm_r"/>
    <origin xyz="-0.13 0 0.53" rpy="0 0 0"/>
    <axis xyz="0 1 0"/>
    <limit lower="-2.6" upper="2.6" effort="100" velocity="10.0"/>
    <dynamics damping="10.0" friction="1.0"/>
  </joint>

  <link name="forearm_r">
    <inertial>
      <mass value="1.2"/>
      <origin xyz="0 0 -0.12"/>
      <inertia ixx="0.0046" ixy="0" ixz="0" iyy="0.0046" iyz="0" izz="0.0012"/>
    </inertial>
    <visual>
      <origin xyz="0 0 -0.12" rpy="1.5708 0 0"/>
      <geometry><capsule radius="0.045" length="0.20"/></geometry>
      <material name="limb_lower"/>
    </visual>
    <collision>
      <origin xyz="0 0 -0.12" rpy="1.5708 0 0"/>
      <geometry><capsule radius="0.045" length="0.20"/></geometry>
    </collision>
  </link>
  <joint name="j_elbow_r" type="revolute">
    <parent link="upperarm_r"/>
    <child link="forearm_r"/>
    <origin xyz="0 0 -0.30" rpy="0 0 0"/>
    <axis xyz="0 1 0"/>
    <limit lower="0" upper="2.7" effort="80" velocity="10.0"/>
    <dynamics damping="8.0" friction="0.8"/>
  </joint>

  <!-- LEFT LEG -->
  <link name="thigh_l">
    <inertial>
      <mass value="5.0"/>
      <origin xyz="0 0 -0.19"/>
      <inertia ixx="0.0603" ixy="0" ixz="0" iyy="0.0603" iyz="0" izz="0.0125"/>
    </inertial>
    <visual>
      <origin xyz="0 0 -0.19" rpy="1.5708 0 0"/>
      <geometry><capsule radius="0.065" length="0.30"/></geometry>
      <material name="limb_upper"/>
    </visual>
    <collision>
      <origin xyz="0 0 -0.19" rpy="1.5708 0 0"/>
      <geometry><capsule radius="0.065" length="0.30"/></geometry>
    </collision>
  </link>
  <joint name="j_thigh_l" type="revolute">
    <parent link="torso"/>
    <child link="thigh_l"/>
    <origin xyz="0.09 0 0.02" rpy="0 0 0"/>
    <axis xyz="0 1 0"/>
    <limit lower="-2.1" upper="1.2" effort="200" velocity="8.0"/>
    <dynamics damping="15.0" friction="1.5"/>
  </joint>

  <link name="shin_l">
    <inertial>
      <mass value="3.5"/>
      <origin xyz="0 0 -0.18"/>
      <inertia ixx="0.0379" ixy="0" ixz="0" iyy="0.0379" iyz="0" izz="0.0070"/>
    </inertial>
    <visual>
      <origin xyz="0 0 -0.18" rpy="1.5708 0 0"/>
      <geometry><capsule radius="0.055" length="0.28"/></geometry>
      <material name="limb_lower"/>
    </visual>
    <collision>
      <origin xyz="0 0 -0.18" rpy="1.5708 0 0"/>
      <geometry><capsule radius="0.055" length="0.28"/></geometry>
    </collision>
  </link>
  <joint name="j_shin_l" type="revolute">
    <parent link="thigh_l"/>
    <child link="shin_l"/>
    <origin xyz="0 0 -0.38" rpy="0 0 0"/>
    <axis xyz="0 1 0"/>
    <limit lower="0" upper="2.8" effort="180" velocity="8.0"/>
    <dynamics damping="12.0" friction="1.2"/>
  </joint>

  <link name="foot_l">
    <inertial>
      <mass value="1.2"/>
      <origin xyz="0.08 0 -0.03"/>
      <inertia ixx="0.0013" ixy="0" ixz="0" iyy="0.0030" iyz="0" izz="0.0028"/>
    </inertial>
    <visual>
      <origin xyz="0.08 0 -0.03"/>
      <geometry><box size="0.24 0.10 0.05"/></geometry>
      <material name="foot"/>
    </visual>
    <collision>
      <origin xyz="0.08 0 -0.03"/>
      <geometry><box size="0.24 0.10 0.05"/></geometry>
      <contact_coefficients mu="1.2" kp="1e7" kd="500"/>
    </collision>
  </link>
  <joint name="j_foot_l" type="revolute">
    <parent link="shin_l"/>
    <child link="foot_l"/>
    <origin xyz="0 0 -0.36" rpy="0 0 0"/>
    <axis xyz="0 1 0"/>
    <limit lower="-0.7" upper="0.7" effort="120" velocity="6.0"/>
    <dynamics damping="10.0" friction="1.0"/>
  </joint>

  <!-- RIGHT LEG -->
  <link name="thigh_r">
    <inertial>
      <mass value="5.0"/>
      <origin xyz="0 0 -0.19"/>
      <inertia ixx="0.0603" ixy="0" ixz="0" iyy="0.0603" iyz="0" izz="0.0125"/>
    </inertial>
    <visual>
      <origin xyz="0 0 -0.19" rpy="1.5708 0 0"/>
      <geometry><capsule radius="0.065" length="0.30"/></geometry>
      <material name="limb_upper"/>
    </visual>
    <collision>
      <origin xyz="0 0 -0.19" rpy="1.5708 0 0"/>
      <geometry><capsule radius="0.065" length="0.30"/></geometry>
    </collision>
  </link>
  <joint name="j_thigh_r" type="revolute">
    <parent link="torso"/>
    <child link="thigh_r"/>
    <origin xyz="-0.09 0 0.02" rpy="0 0 0"/>
    <axis xyz="0 1 0"/>
    <limit lower="-2.1" upper="1.2" effort="200" velocity="8.0"/>
    <dynamics damping="15.0" friction="1.5"/>
  </joint>

  <link name="shin_r">
    <inertial>
      <mass value="3.5"/>
      <origin xyz="0 0 -0.18"/>
      <inertia ixx="0.0379" ixy="0" ixz="0" iyy="0.0379" iyz="0" izz="0.0070"/>
    </inertial>
    <visual>
      <origin xyz="0 0 -0.18" rpy="1.5708 0 0"/>
      <geometry><capsule radius="0.055" length="0.28"/></geometry>
      <material name="limb_lower"/>
    </visual>
    <collision>
      <origin xyz="0 0 -0.18" rpy="1.5708 0 0"/>
      <geometry><capsule radius="0.055" length="0.28"/></geometry>
    </collision>
  </link>
  <joint name="j_shin_r" type="revolute">
    <parent link="thigh_r"/>
    <child link="shin_r"/>
    <origin xyz="0 0 -0.38" rpy="0 0 0"/>
    <axis xyz="0 1 0"/>
    <limit lower="0" upper="2.8" effort="180" velocity="8.0"/>
    <dynamics damping="12.0" friction="1.2"/>
  </joint>

  <link name="foot_r">
    <inertial>
      <mass value="1.2"/>
      <origin xyz="0.08 0 -0.03"/>
      <inertia ixx="0.0013" ixy="0" ixz="0" iyy="0.0030" iyz="0" izz="0.0028"/>
    </inertial>
    <visual>
      <origin xyz="0.08 0 -0.03"/>
      <geometry><box size="0.24 0.10 0.05"/></geometry>
      <material name="foot"/>
    </visual>
    <collision>
      <origin xyz="0.08 0 -0.03"/>
      <geometry><box size="0.24 0.10 0.05"/></geometry>
      <contact_coefficients mu="1.2" kp="1e7" kd="500"/>
    </collision>
  </link>
  <joint name="j_foot_r" type="revolute">
    <parent link="shin_r"/>
    <child link="foot_r"/>
    <origin xyz="0 0 -0.36" rpy="0 0 0"/>
    <axis xyz="0 1 0"/>
    <limit lower="-0.7" upper="0.7" effort="120" velocity="6.0"/>
    <dynamics damping="10.0" friction="1.0"/>
  </joint>

</robot>
